{"version": 2, "builds": [{"src": "client/package.json", "use": "@vercel/static-build", "config": {"distDir": "dist"}}, {"src": "server/index.js", "use": "@vercel/node", "config": {"maxLambdaSize": "50mb"}}], "routes": [{"src": "/api/(.*)", "dest": "/server/index.js"}, {"src": "/(.*)", "dest": "/client/index.html"}], "env": {"NODE_ENV": "production"}, "buildCommand": "cd client && npm install && npm run build", "installCommand": "cd client && npm install && cd ../server && npm install"}